{
    "name": "u2-flighttest",
    "version": "0.5.0",
    "description": "Canvas-based flight systems sandbox for the Universe Unlimited project.",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "workspaces": [
        "src/clients/testbed/*"
    ],
    "scripts": {
        "dev": "npm run dev --workspace=src/clients/testbed/chatgpt-vite",
        "build": "npm run build --workspace=src/clients/testbed/chatgpt-vite",
        "preview": "npm run preview --workspace=src/clients/testbed/chatgpt-vite",
        "lint": "eslint . --ext .ts --max-warnings=0",
        "test": "npm run test --workspace=src/clients/testbed/chatgpt-vite",
        "test:watch": "npm run test:watch --workspace=src/clients/testbed/chatgpt-vite",
        "coverage": "npm run coverage --workspace=src/clients/testbed/chatgpt-vite",
        "start:servers": "bash scripts/start-servers.sh",
        "docs:normalize": "node scripts/normalize-md.mjs",
        "docs:convert": "node scripts/convert-docs.mjs",
        "proto:generate": "pbjs -t static-module -w es6 -o src/clients/testbed/chatgpt-vite/network/proto/ecs.js src/shared/Proto/ecs.proto && pbts -o src/clients/testbed/chatgpt-vite/network/proto/ecs.d.ts src/clients/testbed/chatgpt-vite/network/proto/ecs.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dkomlev/u2.git"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "engines": {
        "node": ">=18"
    },
    "bugs": {
        "url": "https://github.com/dkomlev/u2/issues"
    },
    "homepage": "https://github.com/dkomlev/u2#readme",
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@protobuf-ts/plugin": "^2.11.1",
        "@types/node": "^24.10.1",
        "@typescript-eslint/eslint-plugin": "^8.46.4",
        "@typescript-eslint/parser": "^8.46.4",
        "@vitest/coverage-v8": "^1.6.1",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "jsdom": "^27.2.0",
        "mammoth": "^1.11.0",
        "pdf-parse": "^2.4.5",
        "protobufjs-cli": "^1.1.3",
        "typescript": "^5.9.3",
        "vite": "^5.4.21",
        "vitest": "^1.6.1",
        "ws": "^8.18.0"
    },
    "dependencies": {
        "protobufjs": "^7.5.4"
    }
}
